{
  "name": "@kubb/template-demo",
  "version": "0.0.0",
  "description": "Kubb plugin demo",
  "repository": {
    "type": "git",
    "url": "git://github.com/stijnvanhulle/kubb-plugin-template.git"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.js",
  "browser": "build/index.global.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "jest --runInBand --passWithNoTests --testPathPattern=packages/core/src",
    "test:local": "jest --testPathPattern=packages/core/src --maxWorkers=50%",
    "test:watch": "jest --testPathPattern=packages/core/src --maxWorkers=25% --watch",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@kubb/core": "^1.0.0-alpha.1",
    "@kubb/swagger": "^1.0.0-alpha.1",
    "@kubb/ts-codegen": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-formatjs": "^4.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-cli": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.18"
  }
}
